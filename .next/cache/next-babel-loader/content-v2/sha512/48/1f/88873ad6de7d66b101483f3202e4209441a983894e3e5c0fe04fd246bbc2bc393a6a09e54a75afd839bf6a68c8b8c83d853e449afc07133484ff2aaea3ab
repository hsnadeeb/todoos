={(e) => setNewTodo(e.target.value)}\n          placeholder=\"Add a new todo\"\n        />\n        <button onClick={addTodo}>Add Todo</button>\n      </div>\n      <ul>\n        {todos.map((todo) => (\n          <li key={todo._id}>{todo.text}</li>\n        ))}\n      </ul>\n    </div>\n  );\n};\n\nexport default Home;\n"]},"metadata":{},"sourceType":"module"}